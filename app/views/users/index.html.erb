<%= render 'books/list', book: @book, user: current_user %>
<div class=".container">
<div class=".row">
	<div class="col-lg-8">
		<h2>Books</h2>
		<table class="table">
			<thead>
				<tr>
					<th>image</th>
					<th>name</th>
					<th colspan="3"></th>
				</tr>
			</thead>
			<% @users.each do |user| %>
				<tbody>
					<tr>
						<th><%= attachment_image_tag user,:profile_image,:fill, 100, 100, fallback: "no_image.jpg"%></th>
						<th><%= user.name %></th>
						<th><%= link_to "Show",user_path(user.id) %></th>
					</tr>
				</tbody>
			<% end %>
			</table>
	</div>
</div>
</div>
